{
    "manifest_version":2,
    "name":"GDG Jalandhar",
    "description":"Google Developer Groups (GDGs) are for developers who are interested in Google's developer technology.",
    "version":"1.0.2",
    "icons":{
        "128":"icon_128.png"
    },
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}